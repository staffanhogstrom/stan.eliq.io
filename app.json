{
    "id": "stan.eliq.io",
    "sdk": 2,
    "name": {
        "en": "Eliq.io"
    },
    "description": {
        "en": "Integration with energy meter Eliq"
    },
    "category": "energy",
    "version": "0.0.1",
    "compatibility": ">=1.5.0",
    "author": {
        "name": "Staffan Högström",
        "email": "staffanhogstrom@gmail.com"
    },
    "images": {
        "large": "/assets/images/large.png",
        "small": "/assets/images/small.png"
    },
    "drivers": [
        {
            "id": "eliq",
            "name": {
                "en": "eliq"
            },
            "pair": [
                {
                    "id": "pair"
                }
            ],
            
            "images": {
                "large": "./drivers/eliq/assets/images/large.jpg",
                "small": "./drivers/eliq/assets/images/small.jpg"
            },
            "class": "sensor",
            "capabilities": [
                "meter_power"
            ],
            "mobile": {
                "components": [
                    {
                        "id": "icon"
                    },
                    {
                        "id": "sensor",
                        "capabilities": [
                            "meter_power"
                        ],
                        "options": {
                            "icons": {
                                "meter_power": "./drivers/eliq/assets/plug-energy.svg"
                            }
                        }
                    }
                ]
            }
        }
    ]
}